{
    "version": 2,
    "outputDirectory": "dist",
    "installCommand": "npm install --legacy-peer-deps",
    "build": {
        "env": {
            "NODE_VERSION": "20"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}